.proj-bg-image {
	height: 100%;
	width: 100%;
	-o-object-fit: cover;
	   object-fit: cover;
	position: absolute;
	top: 0;
	overflow: hidden;
	z-index: -2
}

.proj-bg-image:not(.center) {
	-o-object-position: center top;
	   object-position: center top
}

.proj-bg-image.center {
	-o-object-position: center center;
	   object-position: center center
}

.section .intro:not(.remove-blur) .proj-bg-image:before {
