.navbar {
	position: absolute;
	top: 0;
	z-index: 100;
	width: 100%;
	padding: 0 3rem 0 .8rem;
	margin-left: 2.2rem;
	height: 5.5rem;
	-webkit-backdrop-filter: blur(8px) saturate(160%);
	backdrop-filter: blur(8px) saturate(160%)
}

.navbar:after {
	content: "";
	position: absolute;
	top: 0;
	left: -2.2rem;
	width: -webkit-calc(2.2rem - 3px);
	width: -moz-calc(2.2rem - 3px);
	width: calc(2.2rem - 3px);
	height: 5.5rem;
	-webkit-backdrop-filter: blur(8px) saturate(160%);
	backdrop-filter: blur(8px) saturate(160%)
}

.navbar,
.navbar:after {
	background: rgb(251 251 251 / 87%);
	border-bottom: 1px solid #dedede
}

.navbar-white.navbar,
.navbar-white.navbar:after {
	background: rgb(41 41 41 / 87%);
	border-bottom: 1px solid #484848
}

.navbar,
.navbar a,
.navbar a:focus,
.navbar a:hover {
