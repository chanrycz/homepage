}

.pp-scrollable:not([id^=proj-]) .scroll-wrap {
	width: 100%;
	max-height: 100%;
	display: inline-block;
	vertical-align: middle;
	white-space: normal;
	margin-left: -4px;
	padding: 0;
	padding-bottom: 5rem;
	padding-top: 2rem
}

[id^=proj-] .scroll-wrap {
	width: -webkit-calc(100% - 2rem);
	width: -moz-calc(100% - 2rem);
	width: calc(100% - 2rem);
