	transform: translateY(-40px);
	will-change: opacity, transform, backdrop-filter, -webkit-transform, -moz-transform, -o-transform, -webkit-backdrop-filter;
	-webkit-transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease;
	-o-transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease;
	-moz-transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease;
	-webkit-transition: opacity .7s ease, -webkit-transform .7s ease, -webkit-backdrop-filter .7s ease;
	transition: opacity .7s ease, -webkit-transform .7s ease, -webkit-backdrop-filter .7s ease;
	-o-transition: opacity .7s ease, backdrop-filter .7s ease, -o-transform .7s ease;
	-moz-transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease, -moz-transform .7s ease;
	transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease;
	transition: opacity .7s ease, transform .7s ease, backdrop-filter .7s ease, -webkit-transform .7s ease, -moz-transform .7s ease, -o-transform .7s ease, -webkit-backdrop-filter .7s ease
}

.section:not(#home) .intro.animate {
